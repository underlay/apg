{
	"name": "@underlay/apg",
	"version": "0.8.6",
	"description": "Underlay APG utilities",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib"
	],
	"scripts": {
		"test": "npx jest",
		"clean": "rm -rf lib",
		"build": "tsc && npx babel src -d lib --extensions '.ts'",
		"watch-tsc": "tsc --watch",
		"watch-babel": "npx babel src -d lib --extensions '.ts' --watch",
		"dev": "npx npm-run-all --parallel watch-tsc watch-babel"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/underlay/apg.git"
	},
	"author": "Joel Gustafson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/underlay/apg/issues"
	},
	"homepage": "https://github.com/underlay/apg#readme",
	"dependencies": {
		"@types/uuid": "^8.3.0",
		"uuid": "^8.3.2",
		"ziterable": "^1.0.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.10",
		"@babel/core": "^7.12.10",
		"@babel/plugin-proposal-export-namespace-from": "^7.12.1",
		"@babel/plugin-transform-modules-commonjs": "^7.12.1",
		"@babel/plugin-transform-typescript": "^7.12.1",
		"@babel/preset-typescript": "^7.12.7",
		"@types/jest": "^26.0.19",
		"fp-ts": "^2.9.1",
		"io-ts": "^2.2.13",
		"jest": "^26.6.3",
		"n3.ts": "^0.2.5",
		"npm-run-all": "^4.1.5",
		"typescript": "^4.1.3"
	},
	"peerDependencies": {
		"fp-ts": "^2.8.6",
		"io-ts": "^2.2.12",
		"n3.ts": "^0.2.4",
		"typescript": "^4.0.5"
	}
}
